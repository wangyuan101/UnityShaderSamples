			half4 frag ( VertexOutput IN  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				#if defined(ASE_NEEDS_FRAG_WORLD_POSITION)
				float3 WorldPosition = IN.worldPos;
				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				float2 uv_NormalMap = IN.ase_texcoord3.xy * _NormalMap_ST.xy + _NormalMap_ST.zw;
				float3 NormalMap47 = UnpackNormalScale( tex2D( _NormalMap, uv_NormalMap ), 1.0f );
				float3 ase_worldTangent = IN.ase_texcoord4.xyz;
				float3 ase_worldNormal = IN.ase_texcoord5.xyz;
				float3 ase_worldBitangent = IN.ase_texcoord6.xyz;
				float3 tanToWorld0 = float3( ase_worldTangent.x, ase_worldBitangent.x, ase_worldNormal.x );
				float3 tanToWorld1 = float3( ase_worldTangent.y, ase_worldBitangent.y, ase_worldNormal.y );
				float3 tanToWorld2 = float3( ase_worldTangent.z, ase_worldBitangent.z, ase_worldNormal.z );
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				float3 worldRefl41 = reflect( -ase_worldViewDir, float3( dot( tanToWorld0, NormalMap47 ), dot( tanToWorld1, NormalMap47 ), dot( tanToWorld2, NormalMap47 ) ) );
				float3 tanNormal22 = NormalMap47;
				float3 worldNormal22 = float3(dot(tanToWorld0,tanNormal22), dot(tanToWorld1,tanNormal22), dot(tanToWorld2,tanNormal22));
				float3 WorldNormal48 = worldNormal22;
				float dotResult51 = dot( WorldNormal48 , ase_worldViewDir );
				float clampResult54 = clamp( ( 1.0 - dotResult51 ) , 0.0 , 1.0 );
				float FresnelFactor57 = clampResult54;
				float4 ReflectColor46 = ( texCUBE( _ReflectTex, worldRefl41 ) * _ReflectIntensity * ( FresnelFactor57 * FresnelFactor57 ) );
				float fresnelNdotV21 = dot( WorldNormal48, ase_worldViewDir );
				float fresnelNode21 = ( _RimBias + _RimScale * pow( 1.0 - fresnelNdotV21, _RimPower ) );
				float2 uv_EmissionMask = IN.ase_texcoord3.xyz.xy * _EmissionMask_ST.xy + _EmissionMask_ST.zw;
				float4 RimColor37 = ( ( fresnelNode21 + tex2D( _EmissionMask, uv_EmissionMask ).r ) * _RimColor );
				float3 objToView63 = mul( UNITY_MATRIX_MV, float4( IN.ase_texcoord7.xyz, 1 ) ).xyz;
				float3 objToView64 = mul( UNITY_MATRIX_MV, float4( float3( 0,0,0 ), 1 ) ).xyz;
				float3 worldToViewDir80 = mul( UNITY_MATRIX_V, float4( WorldNormal48, 0 ) ).xyz;
				float4 lerpResult85 = lerp( tex2D( _InsideTex, ( ( ( (( objToView63 - objToView64 )).xy * (_TillingAndOffset).xy ) + (_TillingAndOffset).zw ) + ( (worldToViewDir80).xy * _UVDistort ) ) ) , _InsideColor , FresnelFactor57);
				float4 InsideColor71 = lerpResult85;
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( ReflectColor46 + RimColor37 + InsideColor71 ).rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
		}

		
